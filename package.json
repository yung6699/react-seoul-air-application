{
  "name": "react boilerplate",
  "version": "1.0.0",
  "description": "\"node.js react boilerplate\"",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yung6699/react-boilerplate.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build public/bundle.js",
    "development": "export NODE_ENV=development && npm run build:dev && node ./build/index.js",
    "build:server": "babel server --out-dir build",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build": "webpack --config webpack.config.js",
    "start": "export NODE_ENV=production && npm run clean && npm run build:server && npm run build && node ./build/index.js"
  },
  "author": "\"윤태영\"",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.21.0",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  }
}